package nl.basjes.parse.useragent;

public final class Version {

    private static final String git_commit_id_describe_short  = "@git.commit.id.describe-short@";
    private static final String maven_build_timestamp         = "@maven.build.timestamp@";
    private static final String project_version               = "@project.version@";

    public static String getGitCommitIdDescribeShort() {
        return git_commit_id_describe_short;
    }

    public static String getBuildTimestamp() {
        return maven_build_timestamp;
    }

    public static String getProjectVersion() {
        return project_version;
    }

}
